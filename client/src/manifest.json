{
  "name": "FastCup Chrome Extension",

  "manifest_version": 3,

  "version": "0.1",

  "permissions": ["scripting"],

  "host_permissions": ["http://*/", "https://*/"],

  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://csgo.fastcup.net/*"],
      "js": ["content.js"]
    }
  ]
}
